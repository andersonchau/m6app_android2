<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.m6apps"
    android:versionCode="1"
    android:versionName="1.0" >
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />
     
    <application
        android:name="MyApp"
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="六合彩 Apps"
        android:theme="@style/AppTheme" >
         
        <activity
            android:name="com.example.m6apps.DisplayResultActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.m6apps.M6LuckyNumberActivity" />
        <activity android:name="com.example.m6apps.StatisticsActivity" />
        <activity android:name="com.example.m6apps.ChooseBallActivity" />
        
    </application>

</manifest>
